pipeline {
  agent any
  stages {
  stage ('SCM CHECKOUT') {
    steps {
      git branch: 'main', url: 'https://github.com/yassinedarwezi/DevOps.git' }}
      stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        stage('compile'){
            steps {
                sh 'mvn compile -e'
            }
        }
        stage('package'){
            steps {
                sh 'mvn clean package -DskipTests'
            }
        } 
        
          stage('test'){
            steps {
                sh 'mvn test'
            }
        } 
    }}
